<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/sky_700"
        android:orientation="horizontal"
        android:paddingTop="30dp"
        android:paddingBottom="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/bt_back"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginStart="10dp"
            android:background="@drawable/ic_back_24"
            android:foregroundGravity="center"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_menu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/prueba_de_medidor"
            android:textAlignment="center"
            android:textColor="@color/black_500"
            android:textSize="24sp"
            app:flow_verticalAlign="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/bt_exit"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginEnd="10dp"
            android:background="@drawable/ic_exit_44"
            android:foregroundGravity="center"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <!--    END HEADER-->

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="22dp"
                app:cardBackgroundColor="@color/grey"
                app:cardCornerRadius="18dp"
                app:cardElevation="6dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="11dp">

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/tv_date"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/fecha"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:startIconDrawable="@drawable/ic_calendar_24">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/tv_date2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="date" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/tv_operator"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginTop="5dp"
                        android:hint="@string/operador"
                        app:endIconMode="clear_text"
                        app:errorEnabled="true"
                        app:layout_constraintTop_toBottomOf="@+id/tv_date"
                        app:startIconDrawable="@drawable/ic_operator_24">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/tv_operator2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="textPersonName" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/tv_user"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:hint="@string/usuario"
                        app:endIconMode="clear_text"
                        app:layout_constraintTop_toBottomOf="@+id/tv_operator"
                        app:startIconDrawable="@drawable/ic_user_24">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/tv_user2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="textPersonName" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/tv_location"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:hint="@string/direccion"
                        app:endIconMode="clear_text"
                        app:layout_constraintTop_toBottomOf="@+id/tv_user"
                        app:startIconDrawable="@drawable/ic_location_24">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/tv_location2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:inputType="textPostalAddress" />

                    </com.google.android.material.textfield.TextInputLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>
            <!--    END CARDVIEW BASIC INFO-->

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gl_0.33_vertical1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.33" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gl_0.33_vertical2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.66" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tv_mark"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="18dp"
                android:hint="@string/marca"
                app:errorEnabled="true"
                app:layout_constraintEnd_toEndOf="@id/gl_0.33_vertical1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cv_table">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/tv_mark2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textShortMessage" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tv_serial"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="5dp"
                android:hint="@string/serial"
                app:errorEnabled="true"
                app:layout_constraintEnd_toEndOf="@id/gl_0.33_vertical2"
                app:layout_constraintStart_toStartOf="@id/gl_0.33_vertical1"
                app:layout_constraintTop_toBottomOf="@id/cv_table">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/tv_serial2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textShortMessage" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tv_caliber"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="5dp"
                android:hint="@string/calibre"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/gl_0.33_vertical2"
                app:layout_constraintTop_toBottomOf="@id/cv_table">

                <AutoCompleteTextView
                    android:id="@+id/dd_caliber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="none" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tv_metrological_class"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:hint="@string/clase_metrologica"
                app:layout_constraintEnd_toStartOf="@+id/gl_0.33_vertical1"
                app:layout_constraintStart_toStartOf="@+id/gl_0.33_vertical1"
                app:layout_constraintTop_toBottomOf="@id/tv_mark">

                <AutoCompleteTextView
                    android:id="@+id/dd_metrological"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="none" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/tv_new_old"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:hint="@string/nuevo_o_usado"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/gl_0.33_vertical2"
                app:layout_constraintTop_toBottomOf="@id/tv_serial">

                <AutoCompleteTextView
                    android:id="@+id/dd_new_old"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="none" />

            </com.google.android.material.textfield.TextInputLayout>

            <!--   INIT TABLE-->

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="12dp"
                app:cardBackgroundColor="@color/grey"
                app:cardCornerRadius="15dp"
                app:cardElevation="6dp"
                app:contentPadding="4dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_metrological_class">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/glt_0.33_vertical1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.401084" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/glt_0.33_vertical2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.701897" />

                    <Button
                        android:id="@+id/bt_photo"
                        style="@style/Widget.MaterialComponents.Button.Icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/foto"
                        android:textStyle="bold"
                        app:icon="@drawable/ic_camera_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/bt_q1"
                        style="?attr/materialButtonOutlinedStyle"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:text="@string/q1"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/bt_q2"
                        style="?attr/materialButtonOutlinedStyle"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:text="@string/q2"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintHorizontal_bias="0.725"
                        app:layout_constraintStart_toEndOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text_q1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/aforo_text"
                        android:textAlignment="center"
                        android:visibility="invisible"
                        app:layout_constraintEnd_toEndOf="@+id/bt_q1"
                        app:layout_constraintStart_toStartOf="@+id/bt_q1"
                        app:layout_constraintTop_toBottomOf="@+id/bt_q1" />

                    <TextView
                        android:id="@+id/text_q2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/aforo_text"
                        android:textAlignment="center"
                        android:visibility="invisible"
                        app:layout_constraintEnd_toEndOf="@+id/bt_q2"
                        app:layout_constraintStart_toStartOf="@+id/bt_q2"
                        app:layout_constraintTop_toBottomOf="@+id/bt_q2" />

                    <TextView
                        android:id="@+id/text_photo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="invisible"
                        android:text="@string/photo_text"
                        android:textAlignment="center"
                        android:textColor="@color/green"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="@+id/bt_photo"
                        app:layout_constraintStart_toStartOf="@+id/bt_photo"
                        app:layout_constraintTop_toBottomOf="@+id/bt_photo" />

                    <!--A single line-->
                    <TextView
                        android:id="@+id/single_line"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/black_500"
                        android:gravity="center_horizontal"
                        android:textSize="1sp"
                        app:layout_constraintTop_toBottomOf="@+id/text_q2"
                        tools:ignore="SmallSp" />

                    <TextView
                        android:id="@+id/tv_lectura_i"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="5dp"
                        android:gravity="center_vertical"
                        android:text="@string/lectura_inicial_l"
                        android:textAlignment="center"
                        android:textColor="@color/black_500"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/single_line"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_lectura_f"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/lectura_final_l"
                        android:textAlignment="center"
                        android:textColor="@color/black_500"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_lectura_i"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_tiempo"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/tiempo_seg"
                        android:textAlignment="center"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_lectura_f"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_Tepm_agua"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/temp_agua_c"
                        android:textAlignment="center"
                        android:textColor="@color/black_500"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_tiempo"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_Tepm_ambiente"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/temp_ambiente_c"
                        android:textAlignment="center"
                        android:textColor="@color/black_500"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Tepm_agua"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_p_trabajo"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/presi_n_de_trabajo_kpa"
                        android:textAlignment="center"
                        android:textColor="@color/black_500"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_Tepm_ambiente"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_result"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:text="@string/resultado"
                        android:textAlignment="center"
                        android:textColor="@color/green"
                        android:textStyle="bold"
                        android:visibility="invisible"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tv_p_trabajo"
                        tools:ignore="RtlCompat" />
                    <!--A single line-->
                    <TextView
                        android:id="@+id/tv_line2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:background="@color/transparent"
                        android:gravity="center_horizontal"
                        android:textSize="1sp"
                        app:layout_constraintTop_toBottomOf="@+id/tv_p_trabajo"
                        tools:ignore="SmallSp" />

                    <TextView
                        android:id="@+id/tv_q2_process"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:hint="@string/resultado_q2"
                        android:textAlignment="center"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/tv_line2"
                        tools:ignore="RtlCompat" />

                    <TextView
                        android:id="@+id/tv_q1_process"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:gravity="center_vertical"
                        android:hint="@string/resultado_q1"
                        android:textAlignment="center"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@id/glt_0.33_vertical2"
                        app:layout_constraintTop_toBottomOf="@+id/tv_line2"
                        tools:ignore="RtlCompat" />

                    <!--Q2-->
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ti_q2_l_i"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="5dp"
                        android:padding="0dp"
                        android:textAlignment="center"
                        android:theme="@style/textInputStyle"
                        app:endIconDrawable="@drawable/ic_back_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/single_line">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/ti_q2_l_i0"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:inputType="numberDecimal"
                            android:textAlignment="center" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ti_q2_l_f"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginEnd="5dp"
                        android:padding="0dp"
                        android:textAlignment="center"
                        android:theme="@style/textInputStyle"
                        app:endIconDrawable="@drawable/ic_back_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/ti_q2_l_i">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/ti_q2_l_f0"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:inputType="numberDecimal"
                            android:textAlignment="center" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ti_q2_time"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginEnd="5dp"
                        android:padding="0dp"
                        android:textAlignment="center"
                        android:theme="@style/textInputStyle"
                        app:endIconDrawable="@drawable/ic_back_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/ti_q2_l_f">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/ti_q2_time0"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:inputType="numberDecimal"
                            android:textAlignment="center" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ti_q2_t_water"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginEnd="5dp"
                        android:padding="0dp"
                        android:textAlignment="center"
                        android:theme="@style/textInputStyle"
                        app:endIconDrawable="@drawable/ic_back_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/ti_q2_time">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/ti_q2_t_water0"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:inputType="numberDecimal"
                            android:textAlignment="center" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ti_q2_t_environment"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginEnd="5dp"
                        android:padding="0dp"
                        android:textAlignment="center"
                        android:theme="@style/textInputStyle"
                        app:endIconDrawable="@drawable/ic_back_24"
                        app:layout_constraintEnd_toStartOf="@+id/glt_0.33_vertical2"
                        app:layout_constraintStart_toStartOf="@+id/glt_0.33_vertical1"
                        app:layout_constraintTop_toBottomOf="@+id/ti_q2_t_water">

                        <com.google.android.material.textfield.TextInputEditText
